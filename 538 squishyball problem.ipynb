    "# idea of which series length is optimal:\n",
    "\n",
    "series_lengths = range(1, 100, 2)\n",
    "exp_values = [estimate_expected_value(g, 100000) for g in series_lengths]\n",
    "\n",
    "plt.plot(series_lengths, exp_values, 'go')"
